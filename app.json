{
  "expo": {
    "name": "PhpBell For Business",
    "slug": "phpbell-for-business",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icons/android/light/mipmap-xxxhdpi/ic_launcher.png",
    "userInterfaceStyle": "light",
    "scheme": "phpbell-vendor",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shashankgupta01.phpbellforbusiness",
      "icon": "./assets/icons/ios/light/icon-1024.png",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload images.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to add photos to your library when saving images.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record audio for customer support.",
        "NSDocumentsFolderUsageDescription": "This app needs access to your documents to upload files.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to help you select your restaurant location.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to help you select your restaurant location.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.shashankgupta01.phpbellforbusiness",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android/light/mipmap-xxxhdpi/ic_launcher.png",
        "backgroundColor": "#ffffff",
        "monochromeImage": "./assets/icons/android/light/mipmap-xxxhdpi/ic_launcher.png"
      },
      "icon": "./assets/icons/android/light/mipmap-xxxhdpi/ic_launcher.png",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-document-picker",
      "expo-image-picker",
      "expo-location",
      "expo-font",
      "expo-av",
      "expo-updates"
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "eas": {
        "projectId": "28a870e9-36be-4a84-876a-2262fb339e21"
      },
      "API_BASE_URL": "https://phpadmin.phpbells.com/api"
    },
    "owner": "shashankgupta01",
    "runtimeVersion": "1.0.2",
    "jsEngine": "jsc"
  }
}
